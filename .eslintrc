{
    "rules": {
        "strict": [2, "never"],
        "camelcase": [2, {"properties": "never"}],

        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "double"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "ecmaFeatures": {
        "jsx": true
    },
    "plugins": [
        "react"
    ],

    "globals": {
      "require": true,
      "module": true,
      "gon": true,
      "ANavMap": true,
      "AMap": true,
      "React": true
    
    }
}

